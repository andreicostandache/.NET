        <Label x:Name="telefonLabel" Content="Telefon" HorizontalAlignment="Left" Margin="84,378,0,0" VerticalAlignment="Top" VerticalContentAlignment="Center"/>
        <TextBox x:Name="telefonTextBox" HorizontalAlignment="Left" Height="23" Margin="161,378,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" VerticalContentAlignment="Center" Width="140"/>
        <Label x:Name="emailLabel" Content="Email" HorizontalAlignment="Left" Margin="84,427,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.448,10.453" VerticalContentAlignment="Center"/>
        <TextBox x:Name="emailTextBox" HorizontalAlignment="Left" Height="23" Margin="161,427,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" VerticalContentAlignment="Center" Width="250"/>
        <Button x:Name="button" Content="Executa" HorizontalAlignment="Left" Margin="206,475,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
